   "source": [
    "class Solution:\n",
    "    def singleNumber(self, nums: [int]) -> int:\n",
    "        single = nums[0]\n",
    "        for i in range(1, len(nums)):\n",
    "            single = single ^ nums[i]\n",
    "\n",
    "        return single"
   ]
  },
