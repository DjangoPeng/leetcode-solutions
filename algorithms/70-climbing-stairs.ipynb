   "source": [
    "class Solution:\n",
    "    def climbStairs(self, n: int) -> int:\n",
    "        f = [0] * (n+1)\n",
    "        f[0] = 1\n",
    "        f[1] = 1\n",
    "        for i in range(2, n+1):\n",
    "            f[i] = f[i-1] + f[i-2]\n",
    "        return f[n]"
   ]
  },
